    <section class="ftco-section contact-section bg-light">
        

      <div class="container">

      	<div class="row d-flex mb-5 contact-info">
          <div class="w-100"></div>

          <div class="col-md-3 d-flex">
            <% addresses.forEach(function(element) { %>
          	<div class="info bg-white p-4">
	            <p><span><%= element.housename %> </span> <a href="tel://1234567920"></a></p>
	            <p><span><%= element.post %> </span> <a href="tel://1234567920"></a></p>
	            <p><span><%= element.district %> </span> <a href="tel://1234567920"></a></p>
	            <p><span><%= element.pincode %> </span> <a href="tel://1234567920"></a></p>
                <p>  </p>
                <a href="/user-editAddress/<%= element._id %>" class="btn btn-primary">Edit</a>
              <a href="/user-DeleteAddress/<%= element._id %>"class="btn btn-primary">Delete</a>
	          </div>
          </div>
          <% }); %>
                
          
        
   
          
             

          </div>
          <a href="/user-addAddress" class="btn btn-primary">Add address</a>
        </div>
        <div class="row block-9">
          <div class="col-md-6 order-md-last d-flex">
        
          
          </div>

          <div class="col-md-6 d-flex">
      
            <form action="/user-profile" method="post" class="bg-white p-5 contact-form">
              <h2>Personal Information</h2>
              <div class="form-group">
                  <h4>Name</h4>
                  <input type="text" class="form-control" name="name" placeholder="<%= user.name %>">
              </div>
              <div class="form-group">
                  <h4>Email</h4>
                  <input type="text" class="form-control" name="email" placeholder="<%= user.email %>">
              </div>
              <div class="form-group">
                  <h4>Phone</h4>
                  <input type="text" class="form-control" name="phone" placeholder="<%= user.phone %>">
              </div>
              <!-- Add a hidden input field to store the userId -->
              <input type="hidden" name="userId" value="<%= user._id %>">
              <div class="form-group">
                  <a type="submit" href="/user-editprofile?userId=<%= user._id %>" class="btn btn-primary py-3 px-5">Edit</a>
              </div>
              <div class="form-group">
                <a type="submit" href="/user-changePassword" class="btn btn-primary py-3 px-5">Change password</a>
            </div>
          </form>
          
          </div>
        </div>
      </div>
